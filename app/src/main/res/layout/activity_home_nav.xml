<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".HomeNav">

    <LinearLayout
        android:id="@+id/bottomLinLay"
        android:layout_width="390dp"
        android:layout_height="345dp"
        android:layout_marginBottom="80dp"
        android:orientation="vertical"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@id/topLinLay"
        app:layout_constraintVertical_bias="0.628">

        <TextView
            android:id="@+id/event1"
            android:layout_width="386dp"
            android:layout_height="58dp"
            android:layout_gravity="center_vertical"
            android:layout_weight="1"
            android:background="@drawable/nav_border"
            android:fontFamily="monospace"
            android:onClick="carPoolEvent"
            android:paddingTop="20dp"
            android:text=" Carpool from Home to SAP"
            android:textSize="16sp"
            android:textStyle="bold"
            tools:layout_editor_absoluteY="182dp" />


        <TextView
            android:id="@+id/event2"
            android:layout_width="match_parent"
            android:layout_height="52dp"
            android:layout_gravity="center_vertical"
            android:layout_weight="1"
            android:background="@drawable/nav_border"
            android:fontFamily="monospace"
            android:onClick="funDayEvent"
            android:paddingTop="20dp"
            android:text=" Team Fun Day"
            android:textSize="16sp"
            android:textStyle="bold"
            tools:layout_editor_absoluteY="190dp" />

        <TextView
            android:id="@+id/event3"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_gravity="center_vertical"
            android:layout_weight="1"
            android:background="@drawable/nav_border"
            android:fontFamily="monospace"
            android:onClick="funDayCarPool"
            android:paddingTop="20dp"
            android:text=" Fun Day Carpool"
            android:textSize="16sp"
            android:textStyle="bold"
            tools:layout_editor_absoluteY="174dp" />
    </LinearLayout>

    <LinearLayout
        android:layout_width="389dp"
        android:layout_height="55dp"
        android:background="@drawable/nav_border"
        android:orientation="horizontal"
        app:layout_constraintBottom_toBottomOf="parent">

        <ImageButton
            android:id="@+id/imageButton12"
            android:layout_width="83dp"
            android:layout_height="wrap_content"
            android:layout_marginBottom="10dp"
            android:layout_marginLeft="10dp"
            android:layout_marginRight="10dp"
            android:layout_marginTop="10dp"
            android:layout_weight="1"
            android:background="@android:color/transparent"
            android:onClick="goHome"
            android:scaleType="fitCenter"
            app:srcCompat="@drawable/home_icon" />

        <ImageButton
            android:layout_width="83dp"
            android:layout_height="wrap_content"
            android:layout_marginBottom="10dp"
            android:layout_marginLeft="10dp"
            android:layout_marginRight="10dp"
            android:layout_marginTop="10dp"
            android:layout_weight="1"
            android:background="@android:color/transparent"
            android:onClick="goHome"
            android:scaleType="fitCenter"
            app:srcCompat="@drawable/search_icon" />


        <ImageButton
            android:id="@+id/imageButton16"
            android:layout_width="78dp"
            android:layout_height="wrap_content"
            android:layout_marginBottom="10dp"
            android:layout_marginLeft="10dp"
            android:layout_marginRight="10dp"
            android:layout_marginTop="10dp"
            android:layout_weight="1"
            android:background="@android:color/transparent"
            android:onClick="createEvent"
            android:scaleType="fitCenter"
            app:srcCompat="@drawable/add_create" />

        <ImageButton
            android:id="@+id/imageButton13"
            android:layout_width="82dp"
            android:layout_height="wrap_content"
            android:layout_marginBottom="10dp"
            android:layout_marginLeft="10dp"
            android:layout_marginRight="10dp"
            android:layout_marginTop="10dp"
            android:layout_weight="1"
            android:background="@android:color/transparent"
            android:onClick="goDashBoard"
            android:paddingLeft="5dp"
            android:scaleType="fitCenter"
            app:srcCompat="@drawable/robot_icon" />


        <ImageButton
            android:id="@+id/imageButton18"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginBottom="10dp"
            android:layout_marginLeft="10dp"
            android:layout_marginRight="10dp"
            android:layout_marginTop="10dp"
            android:layout_weight="1"
            android:background="@android:color/transparent"
            android:onClick="goUserAccount"
            android:paddingRight="8dp"
            android:scaleType="fitCenter"
            app:srcCompat="@drawable/user_account" />

    </LinearLayout>

    <TextView
        android:id="@+id/textView5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="436dp"
        android:fontFamily="casual"
        android:text="Events  Feed"
        android:textSize="24sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintHorizontal_bias="0.029"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.977" />

    <ImageButton
        android:id="@+id/button3"
        android:layout_width="120dp"
        android:layout_height="80dp"
        android:layout_marginBottom="96dp"
        android:background="#00000000"
        android:scaleType="fitCenter"
        android:text="View Event"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintHorizontal_bias="0.974"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@id/topLinLay"
        app:layout_constraintVertical_bias="0.628"
        app:srcCompat="@drawable/viewevent" />

    <ImageButton
        android:id="@+id/button4"
        android:layout_width="120dp"
        android:layout_height="80dp"
        android:layout_marginBottom="212dp"
        android:background="#00000000"
        android:scaleType="fitCenter"
        android:text="View Event"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintHorizontal_bias="0.974"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@id/topLinLay"
        app:layout_constraintVertical_bias="0.628"
        app:srcCompat="@drawable/viewevent" />

    <ImageButton
        android:id="@+id/button5"
        android:layout_width="120dp"
        android:layout_height="80dp"
        android:layout_marginBottom="324dp"
        android:background="#00000000"
        android:scaleType="fitCenter"
        android:text="View Event"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintHorizontal_bias="0.974"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@id/topLinLay"
        app:layout_constraintVertical_bias="0.628"
        app:srcCompat="@drawable/viewevent" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="324dp"
        android:text="Nov 29 2018, 8:00am"
        android:textSize="15sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintHorizontal_bias="0.028"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@id/topLinLay"
        app:layout_constraintVertical_bias="0.628" />

    <TextView
        android:id="@+id/textView7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="220dp"
        android:text="Nov 30th 2018, 1pm"
        android:textSize="15sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintHorizontal_bias="0.052"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@id/topLinLay"
        app:layout_constraintVertical_bias="0.628" />

    <TextView
        android:id="@+id/textView8"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="104dp"
        android:text="Nov 30th 2018, 12:30pm"
        android:textSize="15sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintHorizontal_bias="0.059"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@id/topLinLay"
        app:layout_constraintVertical_bias="0.628" />

</android.support.constraint.ConstraintLayout>